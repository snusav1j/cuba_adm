= form_for @super_log do |f|
  - if @super_log.errors.any?
    #error_explanation
      %h2= "#{pluralize(@super_log.errors.count, "error")} prohibited this super_log from being saved:"
      %ul
        - @super_log.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :log_id
    = f.number_field :log_id
  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :action
    = f.number_field :action
  .field
    = f.label :log_type
    = f.number_field :log_type
  .actions
    = f.submit 'Save'
